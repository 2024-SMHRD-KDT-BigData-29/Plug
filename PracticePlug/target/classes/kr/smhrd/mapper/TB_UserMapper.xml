<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.smhrd.mapper.TB_UserMapper">

	<insert id="goJoin" parameterType="kr.smhrd.entity.TB_User">
		insert into tb_user values(#{user_id},#{user_pw},#{user_nick},#{user_birthdate},#{user_phone},#{user_addr},#{user_account})
	</insert>

	<select 
		id="goLogin" 
		parameterType="kr.smhrd.entity.TB_User"
		resultType="kr.smhrd.entity.TB_User"
	>
		select * from tb_user where user_id=#{user_id} and user_pw=#{user_pw}
	</select>
	


</mapper>












